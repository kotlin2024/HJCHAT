type Message {
    id: ID,
    content: String
    sender: String
    timestamp: String
}

type Mutation {
    sendMessage(content: String!, chatRoomId: ID!): Message
    createChatRoom(roomName: String!, roomType: String!): ChatRoom
}

type ChatRoom {
    id: ID
    roomName: String
    roomType: String
    createdAt: String
    updatedAt: String
}


type Query {
    getMessages: [Message]
}